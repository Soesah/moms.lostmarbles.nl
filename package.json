{
  "name": "moms.lostmarbles.nl",
  "description": "Moms recipe website",
  "author": {
    "name": "Carl Giesberts",
    "email": "carl@front-crafter.nl",
    "url": "https://www.front-crafter.nl"
  },
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "app": "go run moms.go",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "vue-cli-service build",
    "major": "./bin/deploy.sh major",
    "minor": "./bin/deploy.sh minor",
    "patch": "./bin/deploy.sh patch",
    "deploy": "yarn build && gcloud app deploy app.yaml --project moms-lost-marbles -q"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@vue/cli-plugin-babel": "^4.0.5",
    "@vue/cli-plugin-typescript": "^4.0.5",
    "@vue/cli-service": "^4.0.5",
    "jest": "^24.9.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.4",
    "vue-template-compiler": "^2.6.10"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "prettier": {
    "arrowParens": "always",
    "trailingComma": "all",
    "semi": true,
    "singleQuote": true
  }
}
